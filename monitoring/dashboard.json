        }
      },
      {
        "title": "Conversations (per minute)",
        "xyChart": {
          "dataSets": [
            {
              "plotType": "LINE",
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"logging.googleapis.com/user/marketing_api_conversations\"",
                  "aggregation": {
                    "perSeriesAligner": "ALIGN_RATE",
                    "alignmentPeriod": "60s",
                    "crossSeriesReducer": "REDUCE_SUM"
                  }
                }
