{
  "displayName": "EthosPrompt - Marketing API (STAGING)",
  "dashboardFilters": [],
  "mosaicLayout": {
    "columns": 12,
    "tiles": [
      {
        "height": 4,
        "width": 6,
        "xPos": 0,
        "yPos": 0,
        "widget": {
          "title": "Request Latency (p50/p95/p99)",
          "xyChart": {
            "chartOptions": { "mode": "COLOR" },
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "p50",
                "minAlignmentPeriod": "300s",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"run.googleapis.com/request_latencies\" resource.type=\"run_revision\" resource.label.service_name=\"marketing-api\" resource.label.location=\"australia-southeast1\" project=\"rag-prompt-library-staging\"",
                    "aggregation": { "alignmentPeriod": "300s", "perSeriesAligner": "ALIGN_PERCENTILE_50" }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "p95",
                "minAlignmentPeriod": "300s",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"run.googleapis.com/request_latencies\" resource.type=\"run_revision\" resource.label.service_name=\"marketing-api\" resource.label.location=\"australia-southeast1\" project=\"rag-prompt-library-staging\"",
                    "aggregation": { "alignmentPeriod": "300s", "perSeriesAligner": "ALIGN_PERCENTILE_95" }
                  }
                }
              },
              {
                "plotType": "LINE",
                "legendTemplate": "p99",
                "minAlignmentPeriod": "300s",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"run.googleapis.com/request_latencies\" resource.type=\"run_revision\" resource.label.service_name=\"marketing-api\" resource.label.location=\"australia-southeast1\" project=\"rag-prompt-library-staging\"",
                    "aggregation": { "alignmentPeriod": "300s", "perSeriesAligner": "ALIGN_PERCENTILE_99" }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "height": 4,
        "width": 6,
        "xPos": 6,
        "yPos": 0,
        "widget": {
          "title": "Request Volume (RPM)",
          "xyChart": {
            "chartOptions": { "mode": "COLOR" },
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "Requests/min",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"run.googleapis.com/request_count\" resource.type=\"run_revision\" resource.label.service_name=\"marketing-api\" resource.label.location=\"australia-southeast1\" project=\"rag-prompt-library-staging\"",
                    "secondaryAggregation": {"perSeriesAligner": "ALIGN_RATE"},
                    "aggregation": { "alignmentPeriod": "60s", "perSeriesAligner": "ALIGN_DELTA" }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "height": 4,
        "width": 6,
        "xPos": 0,
        "yPos": 4,
        "widget": {
          "title": "Error Rate (5xx %)",
          "xyChart": {
            "chartOptions": { "mode": "COLOR" },
            "dataSets": [
              {
                "plotType": "LINE",
                "legendTemplate": "5xx %",
                "timeSeriesQuery": {
                  "timeSeriesFilter": {
                    "filter": "metric.type=\"run.googleapis.com/request_count\" resource.type=\"run_revision\" resource.label.service_name=\"marketing-api\" resource.label.location=\"australia-southeast1\" metric.label.response_code_class=\"5xx\" project=\"rag-prompt-library-staging\"",
                    "secondaryAggregation": {"perSeriesAligner": "ALIGN_RATE"},
                    "aggregation": { "alignmentPeriod": "60s", "perSeriesAligner": "ALIGN_DELTA" }
                  }
                }
              }
            ]
          }
        }
      },
      {
        "height": 4,
        "width": 6,
        "xPos": 6,
        "yPos": 4,
        "widget": {
          "title": "Custom Logs: API Request Events (last 1h)",
          "logsPanel": {
            "filter": "resource.type=cloud_run_revision AND jsonPayload.monitoring.event=api_request_end AND resource.labels.service_name=marketing-api"
          }
        }
      }
    ]
  }
}

