{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "dev:debug": "vite --debug",
    "dev:https": "vite --https",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "build:staging": "tsc && vite build --mode staging",
    "build:prod": "tsc && vite build --mode production",
    "build:optimized": "npm run build && npm run optimize:css",
    "build:cdn": "npm run build && npm run optimize:cdn",
    "build:check": "npm run build && npm run check:budget",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 400",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "lint:staged": "lint-staged",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "test:unit:core": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs --run --config vitest.unit-core.config.ts",
    "test:unit:infra": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs --run --config vitest.unit-infra.config.ts",
    "test:unit:misc": "node --max-old-space-size=4096 ./node_modules/vitest/vitest.mjs --run --config vitest.unit-misc.config.ts",
    "test:unit": "npm run test:unit:core && npm run test:unit:infra && npm run test:unit:misc",
    "test:visual": "node --max-old-space-size=6144 ./node_modules/vitest/vitest.mjs run src/test/button-*.test.tsx --pool=forks --maxWorkers=1 --passWithNoTests",
    "test:unit:quarantine": "node --max-old-space-size=6144 ./node_modules/vitest/vitest.mjs --run --config vitest.unit-quarantine.config.ts",
    "test:ci": "npm run test:unit && npm run test:visual && npm run test:unit:quarantine",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:performance": "vitest run --config vitest.performance.config.ts",
    "test:lighthouse": "vitest run src/test/performance/lighthouse.test.ts --config vitest.performance.config.ts",
    "test:load": "vitest run src/test/performance/load.test.ts --config vitest.performance.config.ts",
    "test:perf:all": "npm run test:performance && npm run test:lighthouse && npm run test:load",
    "test:all": "npm run test:ci && npm run test:e2e && npm run test:perf:all",
    "test:emulators:inner": "vitest run --config vitest.emulators.config.ts",
    "test:emulators": "npx firebase-tools emulators:exec --only firestore,auth -- \"npm --prefix frontend run test:emulators:inner\"",
    "emulators:start": "npx firebase-tools emulators:start --only firestore,auth",
    "analyze": "ANALYZE=true npm run build",
    "analyze:serve": "npm run analyze && npx serve dist",
    "analyze:bundle": "npx vite-bundle-analyzer dist/stats.json",
    "size:check": "npm run build && npx bundlesize",
    "size:why": "npx whybundled dist/assets/*.js",
    "performance:test": "npm run build && npm run preview & sleep 5 && node -e \"import('./src/scripts/performance-test.ts').then(m => m.runPerformanceTests())\"",
    "optimize:css": "node scripts/css-optimization.js",
    "optimize:cdn": "node scripts/cdn-optimization.js",
    "optimize:images": "node scripts/image-optimization.js",
    "check:budget": "node scripts/check-performance-budget.js",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --upload.target=temporary-public-storage",
    "lighthouse:server": "lhci server --port=9001",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && npm install",
    "deps:audit": "npm audit",
    "deps:audit:fix": "npm audit fix",
    "deps:outdated": "npm outdated",
    "security:check": "npm audit && npx better-npm-audit audit",
    "quality:check": "npm run lint && npm run type-check && npm run test:ci",
    "quality:fix": "npm run lint:fix && npm run format",
    "pre-commit": "lint-staged && npm run type-check",
    "pre-push": "npm run test:ci && npm run build",
    "deploy:dev": "npm run build:dev && firebase deploy --only hosting:development",
    "deploy:staging": "npm run build:staging && npm run test:ci && firebase deploy --only hosting:staging",
    "deploy:prod": "npm run build:prod && npm run test:ci && firebase deploy --only hosting:production",
    "deploy:preview": "firebase hosting:channel:deploy preview --expires 7d",
    "setup": "npm install && npm run prepare && npm run build",
    "setup:fresh": "rm -rf node_modules package-lock.json && npm install && npm run setup",
    "validate-help-articles": "ts-node scripts/validate-help-articles.ts",
    "seed-help-articles": "ts-node scripts/seed-help-articles.ts",
    "migrate-help-articles": "ts-node scripts/migrate-help-articles.ts",
    "clean": "rm -rf dist coverage .nyc_output test-results.xml",
    "clean:all": "npm run clean && rm -rf node_modules",
    "reset": "npm run clean:all && npm run setup:fresh",
    "docs:build": "typedoc src --out docs/api",
    "docs:serve": "npx serve docs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.6",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/react": "^10.17.0",
    "@sentry/vite-plugin": "^4.3.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.83.0",
    "@types/node": "^24.0.14",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/react-window": "^1.8.8",
    "@types/three": "^0.182.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-autoplay": "^8.6.0",
    "firebase": "^11.10.0",
    "framer-motion": "^12.23.12",
    "fs-extra": "^11.3.0",
    "i18next": "^25.4.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.525.0",
    "node-fetch": "^3.3.2",
    "ogl": "^1.0.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.63.0",
    "react-i18next": "^15.7.1",
    "react-icons": "^5.5.0",
    "react-is": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.28.0",
    "react-syntax-highlighter": "^16.1.0",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^3.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.182.0",
    "web-vitals": "^4.2.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/language-server": "^0.14.26",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.0",
    "eslint": "^9.30.1",
    "eslint-plugin-css": "^0.11.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.0",
    "postcss": "^8.5.6",
    "puppeteer": "^24.15.0",
    "purgecss": "^7.0.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.11",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^6.4.1",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.2.4"
  }
}
