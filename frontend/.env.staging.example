# Firebase Configuration - Staging Template
# Copy to frontend/.env.staging and fill with staging project values
VITE_FIREBASE_API_KEY=your_staging_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=rag-prompt-library-staging.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=rag-prompt-library-staging
VITE_FIREBASE_STORAGE_BUCKET=rag-prompt-library-staging.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
VITE_FIREBASE_APP_ID=1:123456789012:web:abcdef123456
VITE_FIREBASE_MEASUREMENT_ID=G-ABCDEFSTAGE

# Staging Environment
VITE_USE_EMULATORS=false
VITE_APP_NAME=RAG Prompt Library (Staging)
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=staging

# OpenRouter (use free models for testing)
VITE_OPENROUTER_API_KEY=your_staging_openrouter_api_key

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Debug
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info

